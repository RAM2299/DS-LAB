#include<stdio.h>
int main()
{
    int a[10],i,n,se,found=0;
    printf("enter the size of the array:");
    scanf("%d",&n);
    printf("enter the elements in the array");
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    printf("enter the searching elememts:");
    scanf("%d",&se);
    for(i=0;i<n;i++)
    {
        if (a[i]==se)
        {
            printf ("element is not found %d",i);
            found=1;
            break;
        }
    }
    if(found==0)
    {
        printf("element is not found ");
    }
}
