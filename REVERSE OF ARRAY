#include<stdio.h>
int main()
{
        int a[6],b[6],i,n;
printf("enter the number:");
scanf("%d",&n);
printf("enter the array elements");
for(i=0;i<n;i++)
{
    scanf("%d",&a[i]);
}

    for (i=0;i<n;i++)

{
    b[i]=a[n-i-1];
}
printf("reverse of an array:");
for(i=0;i<n;i++)

    printf("%d\n",b[i]);

}
